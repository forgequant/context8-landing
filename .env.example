# context8 Environment Variables
#
# Copy to .env.local for local overrides (gitignored).
# Production values are set in Vercel dashboard.

# Zitadel OIDC authority URL
VITE_ZITADEL_AUTHORITY=http://localhost:8080

# Zitadel SPA client ID (from Zitadel Console → Projects → Applications)
VITE_ZITADEL_CLIENT_ID=

# Zitadel project ID (from Zitadel Console → Projects → <project> → Project Settings)
VITE_ZITADEL_PROJECT_ID=

# ctx8-api backend URL
VITE_API_URL=http://localhost:8081
